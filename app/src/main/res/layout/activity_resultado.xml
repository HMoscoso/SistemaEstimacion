        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <View
        android:id="@+id/view6"
        android:layout_width="120dp"
        android:layout_height="102dp"
        android:layout_marginStart="7dp"
